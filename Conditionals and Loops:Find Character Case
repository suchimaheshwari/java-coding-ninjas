import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
        String a=s.next();
        char ch=a.charAt(0);
        if(ch>=65 && ch<=90)
        {
            System.out.println("1");
        }
        else if(ch>=97 && ch<=122)
        {
            System.out.println("0");
        }
        else
        {
             System.out.println("-1");
        }
    }
        }
        

	

